{
  "name": "dev-portal",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "_eslint": "eslint . --ext .js --ext .jsx --ext .ts --ext .tsx",
    "_prettier": "prettier *.{js,json} \"./!(*node_modules|.next|coverage|.storybook-out|out)/*.{js,jsx,ts,tsx,css,scss}\" \"./!(*node_modules|.next|coverage|.storybook-out|out)/**/*.{js,jsx,ts,tsx,css,scss}\" \"./!(*node_modules|.next|coverage|.storybook-out|out)/**/**/*.{js,jsx,ts,tsx,css,scss}\"",
    "build": "next build && next export",
    "build-storybook": "build-storybook -c .storybook -o .storybook-out",
    "checks": "yarn _prettier --check && yarn _eslint && tsc --noEmit",
    "dev": "rm -rf .next/cache/ && next dev",
    "fix": "yarn _prettier --write && yarn _eslint --fix",
    "postbuild": "next-sitemap",
    "start": "next start",
    "storybook": "start-storybook -p 6006 -c .storybook",
    "test": "jest --clearCache && jest --watch",
    "test:ci": "jest --clearCache && jest --ci",
    "test:coverage": "jest --clearCache && jest --coverage",
    "prepare": "husky install"
  },
  "dependencies": {
    "@babel/core": "^7.8.7",
    "@babel/helper-define-map": "^7.16.7",
    "@babel/helper-regex": "^7.10.5",
    "@babel/plugin-transform-typescript": "^7.8.7",
    "@emotion/babel-plugin": "^11.9.2",
    "@emotion/babel-preset-css-prop": "^11.2.0",
    "@emotion/core": "^10.3.1",
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@mdx-js/loader": "^1.6.22",
    "@mdx-js/react": "^1.6.22",
    "@next/env": "^12.0.8",
    "@storybook/addon-backgrounds": "^6.5.4",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/addon-viewport": "^6.5.4",
    "@storybook/react": "^6.5.4",
    "@testing-library/jest-dom": "^5.1.1",
    "@testing-library/react": "^9.4.0",
    "@types/classnames": "^2.2.9",
    "@types/jest": "^25.1.2",
    "@types/lodash.sortby": "^4.7.6",
    "@types/lodash.uniqby": "^4.7.6",
    "@types/node": "^13.7.0",
    "@types/react": "^17.0.2",
    "@types/react-scroll": "^1.5.4",
    "@types/react-test-renderer": "^17.0.2",
    "@types/scroll-to-element": "^2.0.0",
    "autoprefixer": "^10.4.4",
    "babel-jest": "^25.1.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-import-glob-array": "^0.2.0",
    "babel-preset-react-app": "^9.1.1",
    "body-scroll-lock": "3.0.3",
    "classnames": "^2.2.6",
    "dwolla-mdx-remark": "Dwolla/dwolla-mdx-remark#1.1.0",
    "eslint-plugin-jest": "^23.8.2",
    "github-slugger": "^1.3.0",
    "highlight.js": "^10.7.2",
    "identity-obj-proxy": "^3.0.0",
    "iframe-resizer-react": "1.0.3",
    "isomorphic-unfetch": "^3.0.0",
    "jest": "^25.1.0",
    "jest-transformer-mdx": "^2.1.0",
    "lodash.groupby": "^4.6.0",
    "lodash.map": "^4.6.0",
    "lodash.sortby": "^4.7.0",
    "lodash.uniqby": "^4.7.0",
    "next": "^12.1.6",
    "next-images": "1.3.0",
    "next-plugin-yaml": "^1.0.1",
    "ordinal": "^1.0.3",
    "postcss-flexbugs-fixes": "^5.0.2",
    "react": "^17.0.2",
    "react-collapsible": "^2.8.4",
    "react-dom": "^17.0.2",
    "react-gtm-module": "2.0.8",
    "react-modal-image": "^2.5.0",
