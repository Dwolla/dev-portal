{
  "name": "dev-portal",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "_prettier": "prettier .babelrc *.{js,json} \"./!(*node_modules|.next|coverage)/*.{js,jsx,ts,tsx,css,scss}\" \"./!(*node_modules|.next|coverage)/**/*.{js,jsx,ts,tsx,css,scss}\" \"./!(*node_modules|.next|coverage)/**/**/*.{js,jsx,ts,tsx,css,scss}\"",
    "build": "next build && next export && build-storybook -c .storybook -o .storybook-out",
    "checks": "yarn _prettier --check && tsc --noEmit",
    "dev": "rm -rf .next/cache/ && next dev",
    "fix": "yarn _prettier --write",
    "start": "next start",
    "storybook": "start-storybook -p 6006 -c .storybook",
    "test": "jest --clearCache && jest --watch",
    "test:ci": "jest --clearCache && jest --ci",
    "test:coverage": "jest --clearCache && jest --coverage"
  },
  "dependencies": {
    "@babel/core": "^7.8.7",
    "@babel/plugin-transform-typescript": "^7.8.7",
    "@emotion/babel-preset-css-prop": "^10.0.27",
    "@emotion/core": "^10.0.27",
    "@emotion/styled": "^10.0.27",
    "@mdx-js/loader": "^1.5.5",
    "@next/mdx": "^9.2.1",
    "@storybook/react": "^5.3.14",
    "@testing-library/jest-dom": "^5.1.1",
    "@testing-library/react": "^9.4.0",
    "@types/classnames": "^2.2.9",
    "@types/jest": "^25.1.2",
    "@types/node": "^13.7.0",
    "@types/react": "^16.9.19",
    "babel-jest": "^25.1.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-emotion": "^10.0.27",
    "babel-plugin-import-glob-array": "^0.2.0",
    "babel-preset-react-app": "^9.1.1",
    "classnames": "^2.2.6",
    "identity-obj-proxy": "^3.0.0",
    "isomorphic-unfetch": "^3.0.0",
    "jest": "^25.1.0",
    "jest-transformer-mdx": "^2.1.0",
    "lodash.groupby": "^4.6.0",
    "lodash.map": "^4.6.0",
    "next": "^9.2.1",
    "next-images": "^1.3.0",
    "next-mdx-enhanced": "^2.4.0",
    "prettier": "^1.19.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-test-renderer": "^16.12.0",
    "swr": "^0.1.18",
    "typescript": "^3.7.5"
  }
}
