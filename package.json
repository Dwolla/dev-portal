{
  "name": "dev-portal",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "_eslint": "eslint . --ext .js --ext .jsx --ext .ts --ext .tsx",
    "_prettier": "prettier *.{js,json} \"./!(*node_modules|.next|coverage|.storybook-out|out|.pnpm-store)/*.{js,jsx,ts,tsx,css,scss}\" \"./!(*node_modules|.next|coverage|.storybook-out|out|.pnpm-store)/**/*.{js,jsx,ts,tsx,css,scss}\" \"./!(*node_modules|.next|coverage|.storybook-out|out|.pnpm-store)/**/**/*.{js,jsx,ts,tsx,css,scss}\"",
    "build": "next build && next export",
    "build-storybook": "storybook build -c .storybook -o .storybook-out",
    "checks": "pnpm _prettier --check && pnpm _eslint && tsc --noEmit",
    "dev": "rm -rf .next/cache/ && next dev",
    "fix": "pnpm _prettier --write && pnpm _eslint --fix",
    "postbuild": "next-sitemap",
    "start": "next start",
    "storybook": "storybook dev -p 6006 -c .storybook",
    "test": "jest --clearCache && jest --watch",
    "test:ci": "jest --clearCache && jest --ci",
    "test:coverage": "jest --clearCache && jest --coverage",
    "prepare": "husky install"
  },
  "dependencies": {
    "@babel/core": "^7.21.8",
    "@babel/helper-define-map": "^7.18.6",
    "@babel/helper-regex": "^7.10.5",
    "@babel/plugin-transform-typescript": "^7.21.3",
    "@emotion/babel-plugin": "^11.10.8",
    "@emotion/babel-preset-css-prop": "^11.10.0",
    "@emotion/cache": "^11.10.8",
    "@emotion/react": "^11.10.8",
    "@emotion/server": "^11.10.0",
    "@emotion/styled": "^11.10.8",
    "@mdx-js/loader": "^1.6.22",
    "@mdx-js/react": "^1.6.22",
    "@mui/material": "^5.12.3",
    "@next/env": "^12.3.4",
    "@storybook/addon-backgrounds": "^7.0.8",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/addon-viewport": "^7.0.8",
    "@storybook/react": "^7.0.8",
    "algolia-search-components": "github:Dwolla/algolia-search-components#203de1c",
    "algoliasearch": "^4.17.0",
    "autoprefixer": "^10.4.14",
    "babel-loader": "^9.1.2",
    "babel-plugin-import-glob-array": "^0.2.0",
    "babel-preset-react-app": "^10.0.1",
    "body-scroll-lock": "3.0.3",
    "classnames": "^2.3.2",
    "dwolla-mdx-remark": "github:Dwolla/dwolla-mdx-remark#1.1.0",
    "github-slugger": "^2.0.0",
    "highlight.js": "^11.8.0",
    "identity-obj-proxy": "^3.0.0",
    "iframe-resizer-react": "^1.1.0",
    "lodash.groupby": "^4.6.0",
    "lodash.map": "^4.6.0",
    "lodash.sortby": "^4.7.0",
    "lodash.uniqby": "^4.7.0",
    "next": "^12.3.4",
    "next-images": "1.3.0",
    "next-plugin-yaml": "^1.0.1",
    "ordinal": "^1.0.3",
    "postcss-flexbugs-fixes": "^5.0.2",
    "react": "^18.2.0",
    "react-collapsible": "^2.10.0",
    "react-dom": "^18.2.0",
    "react-gtm-module": "^2.0.11",
    "react-modal-image": "^2.6.0",
    "react-scroll": "^1.8.9",
    "react-select": "5.7.2",
    "react-tabs": "^6.0.1",
    "react-tippy": "^1.4.0",
    "scroll-to-element": "^2.0.3",
    "storybook": "^7.0.8",
    "swr": "^2.1.5"
  },
  "devDependencies": {
    "@algolia/autocomplete-js": "^1.9.2",
    "@storybook/nextjs": "^7.0.8",
    "@svgr/webpack": "^7.0.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.5.1",
    "@types/lodash.sortby": "^4.7.7",
    "@types/lodash.uniqby": "^4.7.7",
    "@types/node": "^18.16.3",
    "@types/react": "^18.2.0",
    "@types/react-scroll": "^1.8.7",
    "@types/react-test-renderer": "^18.0.0",
    "@types/scroll-to-element": "^2.0.2",
    "@types/webpack-env": "^1.18.0",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "babel-jest": "^29.5.0",
    "eslint": "^8.39.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.12",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "lint-staged": "13.2.1",
    "next-sitemap": "^4.0.9",
    "prettier": "^2.8.8",
    "react-test-renderer": "^18.2.0",
    "typescript": "^5.0.4",
    "url-loader": "^4.1.1",
    "webpack": "^5.81.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,css,scss,json,mdx}": "prettier --write",
    "*.{js,jsx,ts,tsx}": "eslint --fix"
  }
}
