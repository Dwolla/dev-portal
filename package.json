{
  "name": "dev-portal",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "_eslint": "eslint . --ext .js --ext .jsx --ext .ts --ext .tsx",
    "_prettier": "prettier .babelrc *.{js,json} \"./!(*node_modules|.next|coverage|.storybook-out|out)/*.{js,jsx,ts,tsx,css,scss}\" \"./!(*node_modules|.next|coverage|.storybook-out|out)/**/*.{js,jsx,ts,tsx,css,scss}\" \"./!(*node_modules|.next|coverage|.storybook-out|out)/**/**/*.{js,jsx,ts,tsx,css,scss}\"",
    "build": "next build && next export",
    "build-storybook": "build-storybook -c .storybook -o .storybook-out",
    "checks": "yarn _prettier --check && yarn _eslint && tsc --noEmit",
    "dev": "rm -rf .next/cache/ && next dev",
    "fix": "yarn _prettier --write && yarn _eslint --fix",
    "start": "next start",
    "storybook": "start-storybook -p 6006 -c .storybook",
    "test": "jest --clearCache && jest --watch",
    "test:ci": "jest --clearCache && jest --ci",
    "test:coverage": "jest --clearCache && jest --coverage"
  },
  "dependencies": {
    "@babel/core": "^7.8.7",
    "@babel/plugin-transform-typescript": "^7.8.7",
    "@emotion/babel-preset-css-prop": "^10.0.27",
    "@emotion/core": "^10.0.27",
    "@emotion/styled": "^10.0.27",
    "@mdx-js/loader": "^1.5.5",
    "@mdx-js/react": "^1.5.7",
    "@next/mdx": "^9.2.1",
    "@storybook/addon-backgrounds": "^5.3.17",
    "@storybook/addon-centered": "5.1.11",
    "@storybook/addon-viewport": "^5.3.18",
    "@storybook/react": "^5.3.14",
    "@testing-library/jest-dom": "^5.1.1",
    "@testing-library/react": "^9.4.0",
    "@types/classnames": "^2.2.9",
    "@types/jest": "^25.1.2",
    "@types/mdx-js__react": "^1.5.1",
    "@types/node": "^13.7.0",
    "@types/react": "^16.9.19",
    "@types/react-scroll": "^1.5.4",
    "@types/react-test-renderer": "^16.9.2",
    "babel-jest": "^25.1.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-emotion": "^10.0.33",
    "babel-plugin-import-glob-array": "^0.2.0",
    "babel-preset-react-app": "^9.1.1",
    "classnames": "^2.2.6",
    "eslint-plugin-jest": "^23.8.2",
    "github-slugger": "^1.3.0",
    "identity-obj-proxy": "^3.0.0",
    "isomorphic-unfetch": "^3.0.0",
    "jest": "^25.1.0",
    "jest-transformer-mdx": "^2.1.0",
    "lodash.groupby": "^4.6.0",
    "lodash.map": "^4.6.0",
    "next": "^9.2.1",
    "next-images": "^1.3.0",
    "next-mdx-enhanced": "^2.4.0",
    "prettier": "^1.19.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-scroll": "^1.7.16",
    "react-select": "3.1.0",
    "react-test-renderer": "^16.12.0",
    "swr": "^0.1.18",
    "typescript": "^3.7.5"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.23.0",
    "@typescript-eslint/parser": "^2.23.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^1.7.0",
    "husky": "4.2.4",
    "lint-staged": "10.1.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,css,scss,json,mdx}": "prettier --write",
    "*.{js,jsx,ts,tsx}": "eslint --fix"
  }
}
